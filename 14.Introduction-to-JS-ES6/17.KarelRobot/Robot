function main(){
   toRightCorner();
   toRightCorner();
   turnLeft();
   turnLeft();
}

function toRightCorner() { 
   move();
   move();
   move();
   move();
   turnLeft();
}

// Another Solution //////////////////////////////////////////

function main(){
   moveFourTimes();
   turnLeft();
   moveFourTimes();
   turnLeft();
   turnLeft();
   turnLeft();
}

function moveFourTimes() { 
   move();
   move();
   move();
   move();
}

